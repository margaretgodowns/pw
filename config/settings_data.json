      "about-hero-image": {
        "type": "about-hero-image",
        "settings": {
          "hero-image-about": "shopify:\/\/shop_images\/PW-Portrait-0002_5c27e244-ceaf-4106-9cff-ec3a741e927f.jpg"
        }
